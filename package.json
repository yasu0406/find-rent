{
  "name": "find-server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run webhook\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.7.5",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.19.2"
  }
}
